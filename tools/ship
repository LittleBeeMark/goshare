rm ../pkg/pb/goshare/*.go
./protoc -I=../protos/goshare --go_out=../pkg/pb/goshare ../protos/goshare/*.proto
./protoc -I=../protos/goshare --go_out=plugins=grpc:../pkg/pb/goshare ../protos/goshare/*.proto

rm ../pkg/pb/ctp/*.go
./protoc -I=../protos/ctp --go_out=../pkg/pb/ctp ../protos/ctp/*.proto

rm ../cpp/ctp/*.h
rm ../cpp/ctp/*.cc
./protoc -I=../protos/ctp --cpp_out=../cpp/ctp ../protos/ctp/*.proto